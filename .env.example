# Example Environment Variables
# Copy this file to .env and fill in your own variables.

# Application variables
APP_NAME="YourAppName"
APP_ENV="local"
APP_KEY=""
APP_DEBUG=true
APP_URL="http://localhost"

# Database settings
DB_CONNECTION="mysql"
DB_HOST="127.0.0.1"
DB_PORT="3306"
DB_DATABASE="your_database"
DB_USERNAME="your_username"
DB_PASSWORD="your_password"

# Mail settings
MAIL_MAILER="smtp"
MAIL_HOST="smtp.mailtrap.io"
MAIL_PORT=2525
MAIL_USERNAME=""
MAIL_PASSWORD=""
MAIL_ENCRYPTION="null"